/*
 * pfxcpy()
 */
#include <stdio.h>

main()
{
	char *gets();			/* get a line from stdin */
	char inbuf[BUFSIZ];		/* input buffer */
	char outbuf[BUFSIZ];		/* output buffer */
	char *pfxcpy();			/* copy string prefix */

	while (gets(inbuf) != NULL)
		printf("%s\n", pfxcpy(outbuf, inbuf));
	exit(0);
}
