# LINDA Editor Key definitions for Ampex 230 terminal.
#
# Function keys send ^A upper case letter ^M (f1=^A @ ^M)
# Shifted function keys send ^A lower case letter ^M (s-f1= ^A ` ^M)

# delete= DEL key
delete=0177

# Define the arrow keys if not already defined by termcap
down =^J
up   =^K
left =^H
right=^L

# look is clear/home (unshifted: ^^; shifted: ESC *)
look=^^
look=^[ 056

# Put accept back on TAB key
accept=^I
# Allow back tab instead:
accept=^[ I

# Unshifted function keys send ^A @ ^M, ^A A ^M, etc.

widen   =^A 0100 ^M
extend  =^A A ^M
first   =^A B ^M
last    =^A C ^M
down    =^A D ^M
up      =^A E ^M
previous=^A F ^M
next    =^A G ^M
copy    =^A H ^M
copy     =^A I ^M
#yank    =^A I ^M
record  =^A J ^M
play=^A K ^M
ignore   =^A L ^M
#join    =^A L ^M
ignore   =^A M ^M 
#global  =^A M ^M
ignore   =^A N ^M
#find    =^A N ^M
undo    =^A O ^M

# Shifted function keys send ^A ` ^M, ^A a ^M, etc.

exit   =^A 0140 ^M
help   =^A a ^M
ignore =^A b ^M 
#literal=^A b ^M
ignore =^A c ^M
ignore =^A d ^M
ignore =^A e ^M
ignore =^A f ^M
ignore =^A g ^M
ignore =^A h ^M
ignore =^A i ^M
ignore =^A j ^M
ignore =^A k ^M
ignore =^A l ^M
ignore =^A m ^M
ignore =^A n ^M
#bfind  =^A n ^M
redo   =^A o ^M
