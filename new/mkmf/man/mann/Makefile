DEST	      = $(DESTDIR)/usr/man/mann

MAKEFILE      = Makefile

PRINT	      = vtroff -man

SRCS	      = mkmf.n

SUFFIX	      = .n:s

VERSION	      = V4BSD

all:;

clean:;

co:;

depend:;

diff:;

index:;

install:;	@echo Installing $(SRCS) in $(DEST)
		@cp $(SRCS) $(DEST)

lint:;

print:		$(SRCS)
		@$(PRINT) $?
		@touch print

tags:;

update:		$(SRCS)
		@echo Installing $? in $(DEST)
		@cp $? $(DEST)
###
